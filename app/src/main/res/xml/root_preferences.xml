<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">
    <Preference
        app:iconSpaceReserved="false"
        app:key="hint_text"
        app:icon="@drawable/ic_baseline_warning_24"
        app:title="@string/hint_top" />

    <EditTextPreference
        app:iconSpaceReserved="false"
        app:defaultValue="-1"
        app:key="volte_width_dp"
        app:title="@string/input_volte_width_title"
        app:summary="@string/input_volte_width_description" />

    <EditTextPreference
        app:iconSpaceReserved="false"
        app:defaultValue="-1"
        app:key="volte_height_dp"
        app:title="@string/input_volte_height_title"
        app:summary="@string/input_volte_height_description" />

    <EditTextPreference
        app:iconSpaceReserved="false"
        app:defaultValue="-1"
        app:key="volte_margin_end_dp"
        app:title="@string/input_volte_margin_end_title"
        app:summary="@string/input_volte_margin_end_description" />

    <ListPreference
        app:defaultValue="-1"
        app:entries="@array/volte_icon_names"
        app:entryValues="@array/volte_icon_values"
        app:key="volte_substitution_icon_index"
        app:title="@string/input_volte_icon_title"
        app:useSimpleSummaryProvider="true" />

    <Preference
        app:iconSpaceReserved="false"
        app:key="hint_text"
        app:icon="@drawable/ic_baseline_warning_24"
        app:title="@string/hint_dangerous_nav_bar_tweak" />

    <EditTextPreference
        app:iconSpaceReserved="false"
        app:defaultValue=""
        app:key="nav_bar_layout_str"
        app:title="@string/input_nav_bar_layout_str_title"
        app:summary="@string/input_nav_bar_layout_str_description" />

    <ListPreference
        app:iconSpaceReserved="false"
        app:defaultValue="-1"
        app:entries="@array/on_off_not_changed"
        app:entryValues="@array/on_off_not_changed_values"
        app:key="double_tap_on_aod_to_wake"
        app:title="@string/input_double_tap_on_aod_to_wake"
        app:useSimpleSummaryProvider="true" />

    <Preference
        app:iconSpaceReserved="false"
        app:key="hint_text"
        app:icon="@drawable/ic_baseline_warning_24"
        app:title="@string/hint_dangerous_reverse_order" />

    <ListPreference
        app:iconSpaceReserved="false"
        app:defaultValue="-1"
        app:entries="@array/on_off_not_changed"
        app:entryValues="@array/on_off_not_changed_values"
        app:key="reverse_launcher_all_app_own_order"
        app:title="@string/reverse_launcher_all_app_own_order"
        app:useSimpleSummaryProvider="true" />

</PreferenceScreen>